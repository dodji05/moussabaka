{% extends 'base_table_admilte.html.twig' %}
{% block ti %} Liste des années{% endblock %}
{% block card_header_title %} Liste des annnées{% endblock %}

{% block tablecontent %}

    <thead>
    <tr>

        <th>N°</th>
        <th>Annee</th>

        <th>actions</th>
    </tr>
    </thead>

    <tbody>
    {% for annee in annees %}
        <tr>
            <td>{{ loop.index }}</td>
            <td>{{ annee.annee }}</td>


            {#                    <td>{{ candidat.active ? 'Yes' : 'No' }}</td>#}
            {#                    <td>{{ candidat.createdAt ? candidat.createdAt|date('Y-m-d H:i:s') : '' }}</td>#}
            <td>
                <a href="{{ path('app_annee_show', {'id': annee.id}) }}">
                    <i class="fas fa-eye" style="color: #28a745">

                    </i>
                </a>
                <a href="{{ path('app_annee_edit', {'id': annee.id}) }}">
                    <i class="fas fa-edit" style="color: #007bff">

                    </i>
                </a>
            </td>
        </tr>
    {% else %}
        <tr>
            <td colspan="7">no records found</td>
        </tr>
    {% endfor %}
    </tbody>


{% endblock %}
